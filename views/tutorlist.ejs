<body>

    <div id="page">
        <!-- Header Section Start -->
        <%- include('./partials/adminheader') %>
        <!-- Header Section End -->

     

       <div class="">
           <div class="shadow">
            <table class="table">
                <thead>
                  <tr>
                    <th scope="col">S/N</th>
                    <th scope="col">Name</th>
                    <th scope="col">Email</th>
                    <th scope="col">Age</th>
                    <th scope="col">Gender</th>
                    <th scope="col">Phone</th>
                    <th scope="col">Years</th>
                    <th scope="col">Teaching</th>
                    <th scope="col">Location</th>
                    <th scope="col">CV</th>

                  </tr>
                </thead>
                <tbody>
                    <% for(var i=0; i < becomeTutor.length; i++){ %>
                        <tr>
                                <th scope="row"><%= i+1 %></th>
                                <td><%= becomeTutor[i].name %></td>
                                <td><a href="mailto:<%= becomeTutor[i].email %>"><%= becomeTutor[i].email %></a></td>
                                <td><%= becomeTutor[i].age %></td>
                                <td><%= becomeTutor[i].gender %></td>
                                <td><%= becomeTutor[i].phone %></td>
                                <td><%= becomeTutor[i].years %></td>
                                <td><%= becomeTutor[i].teaching %></td>
                                <td><%= becomeTutor[i].location %></td>
                                <td><a href="<%= becomeTutor[i].cvLink.slice(8) %>" >Dowwnload CV</a></td>
                              </tr>
                    <% } %>
                  
                  
                  
                </tbody>
              </table>
           </div>
       </div>

    

      


        <!-- Scroll Top Start -->
        <a href="#" class="scroll-top" id="scroll-top">
            <i class="arrow-top fal fa-long-arrow-up"></i>
            <i class="arrow-bottom fal fa-long-arrow-up"></i>
        </a>
        <!-- Scroll Top End -->
